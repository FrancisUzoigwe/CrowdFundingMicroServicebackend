// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model crowdModel {
  email     String  @unique
  password  String?
  id        String @id @default(uuid())
  token     String?
  verified  Boolean?
  secretKey String?
  profile   Json?
  abeg      Json?
}
